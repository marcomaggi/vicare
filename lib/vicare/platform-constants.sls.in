;;;; h_errno friendly interface

(define-syntax h_errno-code
  (lambda (stx)
    (syntax-case stx ()
      ((_ ?code)
       (identifier? #'?code)
       (let ((sym (syntax->datum #'?code)))
	 (define them '(HOST_NOT_FOUND TRY_AGAIN NO_RECOVERY NO_ADDRESS))
	 (if (memq sym them)
	     #'?code
	   (syntax-violation 'h_errno-code "invalid symbol as h_errno code" (syntax->datum stx) sym)))))))

(define-syntax case-h_errno
  (syntax-rules (else)
    ((_ ?h_errno ((?code0 ?code ...) . ?body) ... (else . ?else-body))
     (let ((h_errno ?h_errno))
       (cond ((or (and (fixnum? ?code0) (unsafe.fx= h_errno ?code0))
		  (and (fixnum? ?code)  (unsafe.fx= h_errno ?code))
		  ...)
	      . ?body)
	     ...
	     (else . ?else-body))))
    ((_ ?h_errno ((?code0 ?code ...) . ?body) ...)
     (let ((h_errno ?h_errno))
       (cond ((or (and (fixnum? ?code0) (unsafe.fx= h_errno ?code0))
		  (and (fixnum? ?code)  (unsafe.fx= h_errno ?code))
		  ...)
	      . ?body)
	     ...)))
    ))


